"UnlitGeneric"
{
	"$basetexture" "models\kirillian\zr_rogue_dome/br_fog"

	"$translucent" 1

	"$fade_start" 18000.0		
	//Fade out would begin at ($fade_start) units and end at ($fade_start/var2) units with player distance being inverse to alpha value
	
	"$temp_fade_start" 0.0
	"$var" 1.0
	"$var2" 0.9
	"$var3" 2000.0
	"$distance" 0.0
	"$temp" 0.0
	"$temp_clamped" 0.0
	"$temp2" 0.0
	"$temp_speedGet" 0.0
	
	"$alpha_unclamped" 	0.0
	"$frameSet" 		0
	"$var4multi" 		10.0 //max textures
	"$var5multi" 		6.0 //more agressive

	"Proxies"
	{
		"PlayerProximity"
		{
			"scale" 1.0
			"resultVar" "$distance"
		}

		//we set the dome's ""speed"" to get its size that we scale dynamically
		"EntitySpeed"
		{
			"resultVar" "$temp_speedGet"
		}
		"Multiply"
		{
			"srcVar1" "$fade_start"
			"srcVar2" "$temp_speedGet"
			"resultVar" "$temp_fade_start"
		}
		"Divide"
		{
			"srcVar1" "$distance"
			"srcVar2" "$temp_fade_start"
			"resultVar" "$temp"
		}
		"Multiply"
		{
			"srcVar1" "$alpha_unclamped"
			"srcVar2" "$var5multi"
			"resultVar" "$alpha_unclamped"
		}
		"Clamp"
		{
			"min" 0.0
			"max" "$var3"
			"srcVar1" "$temp"
			"resultVar" "$temp_clamped"
		}
		"Divide"
		{
			"srcVar1" "$var"
			"srcVar2" "$temp_clamped"
			"resultVar" "$temp2"
		}
		"Subtract" //this is used so that the block actually becomes fully transparent at ($temp_fade_start / $var2) units
		{
			"srcVar1" "$temp2"
			"srcVar2" "$var2"
			"resultVar" "$alpha_unclamped"
		}
		"Multiply"
		{
			"srcVar1" "$alpha_unclamped"
			"srcVar2" "$var5multi"
			"resultVar" "$alpha_unclamped"
		}

		"Clamp"
		{
			"min" "0.0"
			"max" "1.0"
			"srcVar1" "$alpha_unclamped"
			"resultVar" "$alpha_unclamped"
		}
		//current 10 frames.
		"Multiply"
		{
			"srcVar1" "$alpha_unclamped"
			"srcVar2" "$var4multi"
			"resultVar" "$frame"
		}
	}
}
